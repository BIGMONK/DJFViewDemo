<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SBorderedImageView">
        <attr name="cborderColor" format="color" />
        <attr name="cborderWidth" format="dimension" />
        <attr name="cshap" format="enum">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="ccorner" format="dimension" />
        <attr name="cleftTopCorner" format="dimension" />
        <attr name="cleftBottomCorner" format="dimension" />
        <attr name="crightTopCorner" format="dimension" />
        <attr name="crightBottomCornet" format="dimension" />
        <attr name="scaleType" format="enum">
            <enum name="fitXY" value="1" />
            <enum name="centerCrop" value="2" />
        </attr>
    </declare-styleable>

</resources>